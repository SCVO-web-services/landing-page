{
  "overrides": [
    {
      "files": ["**/*.{js,jsx,ts,tsx}"],
      "rules": {
        "next/no-html-link-for-pages": ["error", { "pagesDir": "." }]
      },
      "parser": "@babel/eslint-parser",
      "parserOptions": {
        "requireConfigFile": false,
        "ecmaVersion": 2021,
        "sourceType": "module",
        "babelOptions": {
          "presets": ["@babel/preset-env", "@babel/preset-react"]
        }
      }
    }
  ]
}
